{
    "name": "dde16/slate-template",
    "description": "A template to start using Slate.",
    "license": "proprietary",
    "repositories": [
        { "type": "path", "url": "../__dependencies/slate" }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "require": {
        "dde16/slate": "*"
    },
    "options": {
        "symlink": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
